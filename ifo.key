meta:
  id: ifo 
  file-extension: ifo
  endian: be
  ks-debug: true

instances:
  vmg_ifo:
    pos: 0
    type: vmg_ifo

        # c     - cell 
        # adt   - address table
        # admap - address map
        # vobu  - video object unit
        # vobs  - video object set
        # pg    - program
        # pgc   - program chain
        # vts   - video title set
        # vtsm  - video title set menu
        # vmgm  - video manager menu
        # tt    - title track
        # ptt   - part of title track
        # srpt  - search pointer table

types:
#  getbits_state:
#    seq:
#    - id: start
#      type: u1
#    - id: byte_position
#      type: u4
#    - id: bit_position
#      type: u4
#    - id: byte
#      type: u1
#  dvd_reader_stream_cb:
#    seq: []
#  dvd_logger_cb:
#    seq: []
#  dvd_stat:
#    seq:
#    - id: size
#      type: u8
#    - id: nr_parts
#      type: s4
#    - id: parts_size
#      type: u8
#      repeat: expr
#      repeat-expr: 9
  dvd_time:
    seq:
    - id: hour
      type: u1
    - id: minute
      type: u1
    - id: second
      type: u1
    - id: frame_rate
      enum: rate
      type: b2
    - id: frame
      type: b6
    enums:
      rate:
        0: illegal
        1: fps_25
        2: illegal
        3: fps_30
  vm_cmd:
    seq:
    - id: bytes
      type: u1
      repeat: expr
      repeat-expr: 8
  video_attr:
    seq:
    - id: mpeg_version
      type: b2
      enum: mpeg_version
    - id: video_format
      type: b2
      enum: video_format
    - id: aspect_ratio
      type: b2
      enum: aspect_ratio
    - id: disallow_automatic_pan_scan
      type: b1
    - id: disallowautomatic_letterbox
      type: b1
    - id: line21_cc_1
      type: b1
    - id: line21_cc_2
      type: b1
    - id: picture_size_ntsc
      type: b3
      if: video_format == video_format::ntsc
      enum: picture_size_ntsc
    - id: picture_size_pal
      type: b3
      if: video_format == video_format::pal
      enum: picture_size_pal
    - id: letterboxed
      type: b1
    - id: unknown_1
      type: b1
    - id: film_mode
      type: b1
      enum: film_mode
    enums:
      mpeg_version:
        0: mpeg_1
        1: mpeg_2
        2: reserved
        3: reserved
      video_format:
        0: ntsc
        1: pal
        2: reserved
        3: reserved
      aspect_ratio:
        0: ratio_4_3
        1: not_specified
        2: reserved
        3: ratio_16_9
      picture_size_ntsc:
        0: res_720x480
        1: res_704x480
        2: res_352x480
        3: res_352x240
        4: reserved
        5: reserved
        6: reserved
        7: reserved
      picture_size_pal:
        0: res_720x576
        1: res_704x576
        2: res_352x576
        3: res_352x288
        4: reserved
        5: reserved
        6: reserved
        7: reserved
      film_mode:
        0: camera
        1: film
  audio_attr:
    seq:
    - id: audio_format
      type: b3
    - id: multichannel_extension
      type: b1
    - id: lang_type
      type: b2
    - id: application_mode
      type: b2
    - id: quantization
      type: b2
    - id: sample_frequency
      type: b2
    - id: unknown1
      type: b1
    - id: channels
      type: b3
    - id: lang_code
      type: u2
    - id: lang_extension
      type: u1
    - id: code_extension
      type: u1
    - id: unknown3
      type: u1
    - id: app_info
      type: 
        switch-on: application_mode
        cases:
          '0b00': unspecified
          '0b01': karaoke
          '0b10': surround
    types:
      karaoke:
        seq:
        - id: unknown4
          type: b1
        - id: channel_assignment
          type: b3
        - id: version
          type: b2
        - id: mc_intro
          type: b1
        - id: mode
          type: b1
      surround:
        seq:
        - id: unknown5
          type: b4
        - id: dolby_encoded
          type: b1
        - id: unknown6
          type: b3
      unspecified:
        seq:
          - id: zero_1
            size: 1
  subp_attr:
    seq:
    - id: code_mode
      type: b3
    - id: zero1
      type: b3
    - id: type
      type: b2
    - id: zero2
      type: u1
    - id: lang_code
      type: u2
    - id: lang_extension
      type: u1
    - id: code_extension
      type: u1
  user_ops:
    seq:
    - id: zero
      type: b7
    - id: video_pres_mode_change
      type: b1
    - id: karaoke_audio_pres_mode_change
      type: b1
    - id: angle_change
      type: b1
    - id: subpic_stream_change
      type: b1
    - id: audio_stream_change
      type: b1
    - id: pause_on
      type: b1
    - id: still_off
      type: b1
    - id: button_select_or_activate
      type: b1
    - id: resume
      type: b1
    - id: chapter_menu_call
      type: b1
    - id: angle_menu_call
      type: b1
    - id: audio_menu_call
      type: b1
    - id: subpic_menu_call
      type: b1
    - id: root_menu_call
      type: b1
    - id: title_menu_call
      type: b1
    - id: backward_scan
      type: b1
    - id: forward_scan
      type: b1
    - id: next_pg_search
      type: b1
    - id: prev_or_top_pg_search
      type: b1
    - id: time_or_chapter_search
      type: b1
    - id: go_up
      type: b1
    - id: stop
      type: b1
    - id: title_play
      type: b1
    - id: chapter_search_or_play
      type: b1
    - id: title_or_time_play
      type: b1

  pgc:
    seq:
    - id: zero_1
      size: 2
    - id: program_count
      type: u1
    - id: cell_count
      type: u1
    - id: playback_time
      type: dvd_time
    - id: prohibited_ops
      type: user_ops
    - id: audio_control
      type: u2
      repeat: expr
      repeat-expr: 8
    - id: subp_control
      type: u4
      repeat: expr
      repeat-expr: 32
    - id: next_pgc_nr
      type: u2
    - id: prev_pgc_nr
      type: u2
    - id: go_up_pgc_nr
      type: u2
    - id: pg_playback_mode
      type: u1
    - id: still_time
      type: u1
    - id: palette
      type: u4
      repeat: expr
      repeat-expr: 16
    - id: command_tbl_offset
      type: u2
    - id: program_map_offset
      type: u2
    - id: cell_playback_offset
      type: u2
    - id: cell_position_offset
      type: u2
    - id: command_table
      type: command_table
    - id: program_map
      type: program_map
      repeat: expr
      repeat-expr: program_count
    - id: cell_playback
      type: cell_playback
      repeat: expr
      repeat-expr: cell_count
    - id: cell_position
      type: cell_position
      repeat: expr
      repeat-expr: cell_count
    types:
      command_table:
        seq:
        - id: pre_command_count
          type: u2
        - id: post_command_count
          type: u2
        - id: cell_command_count
          type: u2
        - id: end_address_relative_to_command_table
          type: u2
        - id: pre_command
          type: vm_cmd
          repeat: expr
          repeat-expr: pre_command_count
        - id: post_command
          type: vm_cmd
          repeat: expr
          repeat-expr: post_command_count
        - id: cell_command
          type: vm_cmd
          repeat: expr
          repeat-expr: cell_command_count
      program_map:
        seq:
          - id: program_map
            type: u1
      cell_playback:
        seq:
        - id: block_mode
          type: b2
        - id: block_type
          type: b2
        - id: seamless_play
          type: b1
        - id: interleaved
          type: b1
        - id: stc_discontinuity
          type: b1
        - id: seamless_angle
          type: b1
        - id: zero_1
          type: b1
        - id: playback_mode
          type: b1
        - id: restricted
          type: b1
        - id: cell_type
          type: b5
        - id: still_time
          type: u1
        - id: cell_cmd_nr
          type: u1
        - id: playback_time
          type: dvd_time
        - id: first_sector
          type: u4
        - id: first_ilvu_end_sector
          type: u4
        - id: last_vobu_start_sector
          type: u4
        - id: last_sector
          type: u4
      cell_position:
        seq:
        - id: vob_id_nr
          type: u2
        - id: zero_1
          size: 1
        - id: cell_nr
          type: u1
  pgcit:
    seq:
    - id: pgci_srp_count
      type: u2
    - id: zero_1
      size: 2
    - id: last_byte
      type: u4
    - id: pgci_srp
      type: srp
      #      size: last_byte - 8 + 1
      repeat: expr
      repeat-expr: pgci_srp_count
    types:
      srp:
        seq:
        - id: entry_pgc
          type: b1
        - id: zero_1
          type: b3
          #        - id: block_mode
          #          type: b2
          #        - id: block_type
          #          type: b2
        - id: menu_type
          type: b4
        - id: zero_2
          size: 1
        - id: ptl_id_mask
          type: u2
        - id: pgc_start_byte
          type: u4
    instances:
      pgc:
        type: pgc
        repeat: expr
        repeat-expr: pgci_srp_count
        pos: pgci_srp[_index].pgc_start_byte
  pgci_ut:
    seq:
    - id: language_unit_count
      type: u2
    - id: zero_1
      size: 2
    - id: last_byte
      type: u4
    - id: language_unit
      type: language_unit
      repeat: expr
      repeat-expr: language_unit_count
      size: ((last_byte - 8) / language_unit_count)
    types:
      language_unit:
        seq:
        - id: language_code
          type: str
          encoding: ASCII
          size: 2
        - id: language_extension
          type: u1
        - id: exists_in_menu
          type: b1
        - id: language_start_byte
          type: u4
        - id: pgcit
          type: pgcit
          size-eos: true

  c_adt:
    seq:
    - id: vob_count
      type: u2
    - id: zero_1
      size: 2
    - id: last_byte
      type: u4
    - id: cell_address_table
      type: cell_address
    types:
      cell_address:
        seq:
        - id: vob_id
          type: u2
        - id: cell_id
          type: u1
        - id: zero_1
          size: 1
        - id: start_sector
          type: u4
        - id: last_sector
          type: u4
  vobu_admap:
    seq:
    - id: last_byte
      type: u4
    - id: vobu_start_sectors
      type: u4
      
  tt_srpt:
    # http://dvd.sourceforge.net/dvdinfo/ifo_vmg.html#tt
    seq:
    - id: srpt_count
      type: u2
    - id: zero_1
      size: 2
    - id: last_byte
      type: u4
    - id: title
      type: title_info
      repeat: expr
      repeat-expr: srpt_count
    types:
      title_info:
        enums:
          jcl_command:
            # Jump/Link/Call
            # only_in
            0x0: none
            0x1: invalid
            0x3: button
            0x5: pre_or_post 
            0x7: button_and_pre_or_post
            0x9: cell
            0xB: cell_and_button
            0xD: cell_and_pre_or_post
            0xF: everywhere
        seq:
        - id: zero_1
          type: b1
        - id: multi_or_random_pgc_title
          type: b1
        - id: jlc_cmd
          type: b4
          enum: jcl_command
        - id: chapter_search_or_play
          type: b1
        - id: title_or_time_play
          type: b1
        - id: angle_count
          type: u1
        - id: ptt_count
          type: u2
          # chapters
        - id: parental_id
          type: u2
        - id: title_set_nr
          type: u1
          # VTSN
        - id: vts_ttn
          type: u1
          # VTS_TTN
        - id: title_set_sector
          type: u4
          # referenced to whole disk

  ptl_mait:
    # http://dvd.sourceforge.net/dvdinfo/ifo_vmg.html#mait
    seq:
    - id: country_count
      type: u2
    - id: nr_of_vtss
      type: u2
    - id: last_byte
      type: u4
    - id: countries
      type: ptl_mait_country
    types:
      ptl_mait_country:
        seq:
        - id: country_code
          type: u2
        - id: zero_1
          size: 2
        - id: pf_ptl_mai_start_byte
          type: u2
        - id: zero_2
          size: 2
        - id: pf_ptl_mai
          type: pf_level
        types:
          pf_level:
            seq:
              - id: pf_level
                type: u2
                repeat: expr
                repeat-expr: 8
                
  vts_atrt:
    params:
      - id: offse
        type: u4    
    # http://dvd.sourceforge.net/dvdinfo/ifo_vmg.html#atrt
    seq:
    - id: vts_count
      type: u2
    - id: zero_1
      size: 2
    - id: last_byte
      type: u4
    - id: vts_atrt_offsets
      type: u4
      repeat: expr
      repeat-expr: vts_count
    instances:
      vts:
        type: vts_attribute
        repeat: expr
        repeat-expr: vts_count
        pos: (offse * 0x800) + vts_atrt_offsets[_index]
    types:
      vts_attribute:
        seq:
        - id: last_byte
          type: u4
        - id: vts_cat
          type: u4
        - id: vtsm_vobs_attr
          type: video_attr
        - id: zero_1
          size: 1
        - id: nr_of_vtsm_audio_streams
          type: u1
        - id: vtsm_audio_attr
          type: audio_attr
        - id: zero_2
          type: audio_attr
          repeat: expr
          repeat-expr: 7
        - id: zero_3
          size: 16
        - id: zero_4
          size: 1
        - id: nr_of_vtsm_subp_streams
          type: u1
        - id: vtsm_subp_attr
          type: subp_attr
        - id: zero_5
          type: subp_attr
          repeat: expr
          repeat-expr: 27
        - id: zero_6
          size: 2
        - id: vtstt_vobs_video_attr
          type: video_attr
        - id: zero_7
          size: 1
        - id: nr_of_vtstt_audio_streams
          type: u1
        - id: vtstt_audio_attr
          type: audio_attr
          repeat: expr
          repeat-expr: 8
        - id: zero_8
          size: 16
        - id: zero_9
          size: 1
        - id: nr_of_vtstt_subp_streams
          type: u1
        - id: vtstt_subp_attr
          type: subp_attr
          repeat: expr
          repeat-expr: 32

  txtdt_mgi:
    seq:
    - id: disc_name
      type: s1
      repeat: expr
      repeat-expr: 12
    - id: zero_1
      size: 2
    - id: nr_of_language_units
      type: u2
    - id: last_byte
      type: u4
    - id: lu
      type: txtdt_lu
    types:
      txtdt_lu:
        seq:
        - id: lang_code
          type: u2
        - id: zero_1
          size: 1
        - id: char_set
          type: u1
        - id: txtdt_start_byte
          type: u4
        - id: txtdt
          type: txtdt
        types:
          txtdt:
            seq:
            - id: last_byte
              type: u4
            - id: offsets
              type: u2
              repeat: expr
              repeat-expr: 100
            - id: unknown
              type: u2
            - id: zero_1
              size: 2
            - id: type_of_info
              type: u1
            - id: unknown1
              type: u1
            - id: unknown2
              type: u1
            - id: unknown3
              type: u1
            - id: unknown4
              type: u1
            - id: unknown5
              type: u1
            - id: offset
              type: u2
            - id: text
              type: s1
              repeat: expr
              repeat-expr: 12
  vtsi_mat:
    seq:
    - id: vts_identifier
      type: str
      encoding: ASCII
      size: 12
    - id: vts_last_sector
      type: u4
    - id: zero_1
      size: 12
    - id: vtsi_last_sector
      type: u4
    - id: zero_2
      size: 1
    - id: specification_version
      type: u1
    - id: vts_category
      type: u4
    - id: zero_3
      size: 2
    - id: zero_4
      size: 2
    - id: zero_5
      size: 1
    - id: zero_6
      size: 19
    - id: zero_7
      size: 2
    - id: zero_8
      size: 32
    - id: zero_9
      size: 8
    - id: zero_10
      size: 24
    - id: vtsi_last_byte
      type: u4
    - id: zero_11
      size: 4
    - id: zero_12
      size: 56
    - id: vtsm_vobs
      type: u4
    - id: vtstt_vobs
      type: u4
    - id: vts_ptt_srpt
      type: u4
    - id: vts_pgcit
      type: u4
    - id: vtsm_pgci_ut
      type: u4
    - id: vts_tmapt
      type: u4
    - id: vtsm_c_adt
      type: u4
    - id: vtsm_vobu_admap
      type: u4
    - id: vts_c_adt
      type: u4
    - id: vts_vobu_admap
      type: u4
    - id: zero_13
      size: 24
    - id: vtsm_video_attr
      type: video_attr
    - id: zero_14
      size: 1
    - id: nr_of_vtsm_audio_streams
      type: u1
    - id: vtsm_audio_attr
      type: audio_attr
    - id: zero_15
      type: audio_attr
      repeat: expr
      repeat-expr: 7
    - id: zero_16
      size: 17
    - id: nr_of_vtsm_subp_streams
      type: u1
    - id: vtsm_subp_attr
      type: subp_attr
    - id: zero_17
      type: subp_attr
      repeat: expr
      repeat-expr: 27
    - id: zero_18
      size: 2
    - id: vts_video_attr
      type: video_attr
    - id: zero_19
      type: u1
    - id: nr_of_vts_audio_streams
      type: u1
    - id: vts_audio_attr
      type: audio_attr
      repeat: expr
      repeat-expr: 8
    - id: zero_20
      size: 16
    - id: nr_of_vts_subp_streams
      type: u2
    - id: vts_subp_attr
      type: subp_attr
      repeat: expr
      repeat-expr: 32
    - id: zero_21
      size: 2
    - id: vts_mu_audio_attr
      type: multichannel_ext
      repeat: expr
      repeat-expr: 8
    types:
      multichannel_ext:
        seq:
          - id: zero1
            type: b7
          - id: ach0_gme
            type: b1
          - id: zero2
            type: b7
          - id: ach1_gme
            type: b1
          - id: zero3
            type: b4
          - id: ach2_gv1e
            type: b1
          - id: ach2_gv2e
            type: b1
          - id: ach2_gm1e
            type: b1
          - id: ach2_gm2e
            type: b1
          - id: zero4
            type: b4
          - id: ach3_gv1e
            type: b1
          - id: ach3_gv2e
            type: b1
          - id: ach3_gm1e
            type: b1
          - id: ach3_se2e
            type: b1
          - id: zero5
            type: b4
          - id: ach4_gv1e
            type: b1
          - id: ach4_gv2e
            type: b1
          - id: ach4_gm1e
            type: b1
          - id: ach4_se2e
            type: b1
          - id: zero6
            type: u1
            repeat: expr
            repeat-expr: 19
  vmgi_mat:
    # Video Manager Information Management Table
    seq:
    - id: vmg_identifier
      type: str
      encoding: ASCII
      size: 12
    - id: vmg_last_sector
      type: u4
    - id: zero_1
      size: 12
    - id: vmgi_last_sector
      type: u4
    - id: specification_version
      type: version_number
    - id: vmg_category
      type: u4
    - id: vmg_volume_count
      type: u2
    - id: vmg_this_volume_nr
      type: u2
    - id: disc_side
      type: u1
    - id: zero_2
      size: 19
    - id: vmg_title_set_count
      type: u2
    - id: provider_identifier
      size: 32
      encoding: ASCII
      type: str
    - id: vmg_pos_code
      type: u8
    - id: zero_3
      size: 24
    - id: vmgi_last_byte
      type: u4
    - id: first_play_pgc
      type: u4
    - id: zero_4
      size: 56
    - id: vmgm_vobs
      type: u4
    - id: tt_srpt
      type: u4
    - id: vmgm_pgci_ut
      type: u4
    - id: ptl_mait
      type: u4
    - id: vts_atrt
      type: u4
    - id: txtdt_mgi
      type: u4
    - id: vmgm_c_adt
      type: u4
    - id: vmgm_vobu_admap
      type: u4
    - id: zero_5
      size: 32
    - id: vmgm_video_attr
      type: video_attr
    - id: vmgm_audio_stream_count
      type: u2
    - id: vmgm_audio_attr
      type: audio_attr
      repeat: expr
      repeat-expr: 8
    - id: zero_6
      size: 16
    - id: vmgm_subp_stream_count
      type: u2
    - id: vmgm_subp_attr
      type: subp_attr
    - id: zero_7
      size: 164
    types:
      version_number:
        seq:
          - id: reserved
            size: 1
          - id: major_version_number
            type: b4
          - id: minor_version_number
            type: b4
  vts_ptt_srpt:
    seq:
    - id: srpt_count
      type: u2
    - id: zero_1
      size: 2
    - id: last_byte
      type: u4
    - id: title
      type: ttu
    - id: ttu_offset
      type: u4
    types:
